{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for a React project",
  "author": "sirchras (Chris L)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirchras/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/sirchras/react-boilerplate/issues"
  },
  "homepage": "https://github.com/sirchras/react-boilerplate#readme",
  "keywords": [
    "boilerplate",
    "fullstack",
    "react"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "build": "npm run webpack",
    "client": "webpack serve --config ./client/webpack.config.js",
    "server": "node server",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "authenticare": "^0.4.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.95.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@reduxjs/toolkit": "^1.5.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.19.0",
    "eslint-config-eda": "^0.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.3",
    "sqlite3": "^5.0.2",
    "styled-components": "^5.2.1",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
